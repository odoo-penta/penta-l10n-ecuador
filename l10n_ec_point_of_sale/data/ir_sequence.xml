<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data noupdate="1">

        <record id="seq_cash_session" model="ir.sequence">
            <field name="name">Cash Session Sequence</field>
            <field name="code">cash.session</field>
            <field name="prefix">SE-</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="seq_cash_session_movement" model="ir.sequence">
            <field name="name">Cash Session Movement Sequence</field>
            <field name="code">cash.session.movement</field>
            <field name="prefix">MOV-</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

    </data>
</odoo>
