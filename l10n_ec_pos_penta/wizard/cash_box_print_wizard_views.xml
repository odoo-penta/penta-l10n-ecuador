<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_cash_box_print_wizard_form" model="ir.ui.view">
        <field name="name">cash.box.print.wizard.form</field>
        <field name="model">cash.box.print.wizard</field>
        <field name="arch" type="xml">
            <form string="Print Cash Box Report">
                <group>
                    <field name="cash_box_session_id" invisible="1"/>
                    <field name="report_type"/>
                </group>

                <footer>
                    <button string="Print"
                            type="object"
                            name="action_print"
                            class="btn-primary"/>
                    <button string="Cancel"
                            class="btn-secondary"
                            special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

</odoo>
