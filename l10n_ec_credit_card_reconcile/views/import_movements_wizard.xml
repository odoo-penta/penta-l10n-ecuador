<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_import_movements_wizard_form" model="ir.ui.view">
        <field name="name">import.movements.wizard.form</field>
        <field name="model">import.movements.wizard</field>
        <field name="arch" type="xml">
           <form string="Importar Movimientos" create="false">
                <group>
                    <field name="credit_card_reconcile_id" invisible="1"/>
                    <field name="file_name" readonly="1"/>
                    <field name="import_file" filename="file_name"/>
                </group>
                <footer>
                    <button string="Cancelar" class="btn-secondary" special="cancel" type="object"/>
                    <button string="Importar" class="btn-primary" name="action_importar" type="object"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
